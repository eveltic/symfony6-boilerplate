<!DOCTYPE html>

<title>User index</title>

{% block body %}
    <h1>User index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
                <th>Roles</th>
                <th>Password</th>
                <th>Uuid</th>
                <th>State</th>
                <th>Data</th>
                <th>IsVerified</th>
                <th>CreatedAt</th>
                <th>AuthCode</th>
                <th>EmailAuthEnabled</th>
                <th>TrustedVersion</th>
                <th>BackupCodes</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <td>{{ user.id }}</td>
                <td>{{ user.email }}</td>
                <td>{{ user.roles ? user.roles|json_encode : '' }}</td>
                <td>{{ user.password }}</td>
                <td>{{ user.uuid }}</td>
                <td>{{ user.state }}</td>
                <td>{{ user.data ? user.data|json_encode : '' }}</td>
                <td>{{ user.isVerified ? 'Yes' : 'No' }}</td>
                <td>{{ user.createdAt ? user.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ user.authCode }}</td>
                <td>{{ user.emailAuthEnabled ? 'Yes' : 'No' }}</td>
                <td>{{ user.trustedVersion }}</td>
                <td>{{ user.backupCodes ? user.backupCodes|json_encode : '' }}</td>
                <td>
                    <a href="{{ path('app_test_show', {'id': user.id}) }}">show</a>
                    <a href="{{ path('app_test_edit', {'id': user.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="14">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_test_new') }}">Create new</a>
{% endblock %}
